<?xml version="1.0"?>
<preset version="11">
    <type>image</type>
    <comment>creates mezzanines for VFX shots to run files off of</comment>
    <video>
        <fileType>Dpx</fileType>
        <codec>1316896</codec>
        <codecProfile></codecProfile>
        <namePattern>&lt;name&gt;.</namePattern>
        <transferCharacteristic>-1</transferCharacteristic>
        <colorimetricSpecification>-1</colorimetricSpecification>
        <includeAlpha>False</includeAlpha>
        <overwriteWithVersions>False</overwriteWithVersions>
        <posterFrame>False</posterFrame>
        <useFrameAsPoster>1</useFrameAsPoster>
        <resize>
            <resizeType>fit</resizeType>
            <resizeFilter>lanczos</resizeFilter>
            <width>0</width>
            <height>0</height>
            <bitsPerChannel>12</bitsPerChannel>
            <numChannels>3</numChannels>
            <floatingPoint>False</floatingPoint>
            <bigEndian>False</bigEndian>
            <pixelRatio>1</pixelRatio>
            <scanFormat>P</scanFormat>
        </resize>
    </video>
    <name>
        <framePadding>4</framePadding>
        <startFrame>1000</startFrame>
        <frameIndex>0</frameIndex>
    </name>
</preset>